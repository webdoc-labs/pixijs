{
  "source": {
    "include": [
      "projects"
    ],
    "includePattern": [
      "\\.ts"
    ],
    "excludePattern": [
      "node_modules",
      "pixi-ui",
      "pixi-filters",
      "data-half-edge-mesh",
      "bvh",
      "filter-fft",
      "system-g",
      "ooo-renderer",
      "diagnostic-geometry",
      "pixi-essentials/packages/types",
      "tilemap/index.d.ts",
      "tilemap/demo",
      "tilemap/test"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs"
  },
  "template": {
    "applicationName": "PixiJS",
    "siteDomain": "http://pixijs.webdoclabs.com/",
    "readme": "./README.md",
    "outputSourceFiles": true,
    "integrations": {
      "search": {
        "provider": "algolia",
        "apiKey": "36884c184efbfebb0616a5605c5d6306",
        "appId": "VLNI9I2J4E",
        "indexName": "pixijs"
      }
    },
    "stylesheets": [
      "./stylesheets/index.css",
      "./stylesheets/header.css",
      "./stylesheets/md.css"
    ],
    "meta": {
      "og:title": "PixiJS API Documentation",
      "og:description": "Documentation for all of the PixiJS ecosystem, in one place",
      "og:image": "https://camo.githubusercontent.com/dd5f0c8ce84c09599ebd30c7a48076b3ca4ef7d1f36aed96c64952d3432942a1/68747470733a2f2f706978696a732e646f776e6c6f61642f706978696a732d62616e6e65722d6e6f2d76657273696f6e2e706e67",
      "og:site": "{{url}}"
    }
  }
}
